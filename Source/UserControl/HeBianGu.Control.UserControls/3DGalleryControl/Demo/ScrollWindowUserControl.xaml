<UserControl x:Class="HeBianGu.Control.UserControls._3DGalleryControl.Demo.ScrollWindowUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeBianGu.Control.UserControls._3DGalleryControl.Demo"
             mc:Ignorable="d" 
             xmlns:my="clr-namespace:linhang.Controls"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid Width="600" Height="500">
            <my:ImageScrollView HorizontalAlignment="Center" x:Name="imageScrollView1" VerticalAlignment="Top" />
            <RadioButton Content="效果1（可连跳）" Height="16" HorizontalAlignment="Left" Margin="38,389,0,0" Name="radioButton1" VerticalAlignment="Top" GroupName="a" Checked="radioButton1_Checked" IsChecked="True" />
            <RadioButton Content="效果2(单跳)" Height="16" HorizontalAlignment="Right" Margin="0,389,313,0" Name="radioButton2" VerticalAlignment="Top" GroupName="a" Checked="radioButton2_Checked" />
            <CheckBox Content="允许持续滑动" Height="16" HorizontalAlignment="Right" Margin="0,389,218,0" Name="checkBox1" VerticalAlignment="Top" Click="checkBox1_Click" IsEnabled="False" />
        </Grid>
    </Grid>
</UserControl>
