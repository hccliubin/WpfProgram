<UserControl x:Class="SystemFolderModule.View.NavigationBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SystemFolderModule.View"
              xmlns:base="clr-namespace:HeBianGu.Base.WpfBase;assembly=HeBianGu.Base.WpfBase"
           xmlns:c="clr-namespace:HeBianGu.General.WpfControlLib;assembly=HeBianGu.General.WpfControlLib" 
             xmlns:m="clr-namespace:HeBianGu.General.ModuleManager;assembly=HeBianGu.General.ModuleManager"
             mc:Ignorable="d" >
    <UserControl.Resources>
        <ResourceDictionary Source="pack://application:,,,/HeBianGu.General.ModuleManager;component/ModuleResource/ModuleResource.xaml" />
    </UserControl.Resources>

    <Grid>
        <c:FButton x:Name="btn_bar" FIcon="&#xe608;" Content="系统资源" base:ControlAttachProperty.IsBuzy="{Binding IsBusyFlag}" ToolTip="{Binding ElementName=btn_bar, Path=Content}" Style="{StaticResource ModuleFButtonStyle}" Click="NavigateToEmailRadioButton_Click"  />
    </Grid>
</UserControl>
